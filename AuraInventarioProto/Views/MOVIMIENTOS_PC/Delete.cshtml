@model AuraInventarioProto.Models.MOVIMIENTOS_PC

@{
    ViewBag.Title = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Inhabilitar</h2>

<h3>¿Esta Seguro de que desear inhabilitar este registro?</h3>
<div class="well bs-component animated fadeInLeftBig">
    <h4>Movimientos de Pc</h4>
    <hr />
    <dl class="dl-horizontal ">
        <dt>
            @Html.DisplayNameFor(model => model.RUT_USUARIO)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RUT_USUARIO)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ID_PC)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ID_PC)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TIPO_MOV)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TIPO_MOV)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.FECHA_MOV)
        </dt>

        <dd>
            @Html.ValueFor(model => model.FECHA_MOV, "{0:dd-MM-yyyy}")
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.OBS)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OBS)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Inhabilitar" class="btn btn-danger" /> |
            @Html.ActionLink("Volver al Listado", "Index")
