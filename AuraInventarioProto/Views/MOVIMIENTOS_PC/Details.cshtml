@model AuraInventarioProto.Models.MOVIMIENTOS_PC

@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div class="well bs-component">
    <h4>Movimientos de Pc</h4>
    <hr />
    <dl class="dl-horizontal ">
        <dt>
            Rut:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RUT_USUARIO)
        </dd>

        <dt>
            Id Pc:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ID_PC)
        </dd>

        <dt>
            Tipo Movimiento:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TIPO_MOV)
        </dd>

        <dt>
            Fecha Movimiento:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FECHA_MOV)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.OBS)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OBS)
        </dd>

    </dl>
</div>
<p>
    @if (Session["UserRole"].ToString() != "ReadOnly") {
        @Html.ActionLink("Editar", "Edit", new { id = Model.ID })        <text>|</text>
}
    @Html.ActionLink("Volver al Listado", "Index")
</p>
