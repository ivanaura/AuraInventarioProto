@using (Html.BeginForm("UploadExcel", "USUARIOS", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" })) { 
    <div id="myModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Ingreso Masivo de Usuarios</h4>
                </div>
                <div class="modal-body">
                    <p>Descargar Formato:</p>
                    <a href="/USUARIOS/DownloadExcel/"><img src="~/excel.ico" width="25" height="25" title="Download Excel format" alt="excel" /></a>
                    <hr />
                    <p>Cargar Excel:</p>
                    <input type="file" id="FileUpload" name="FileUpload" class="" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                    <button type="submit" class="btn btn-primary">Guardar Cambios</button>
                </div>
            </div>
        </div>
    </div>
}

